      "param_matrix": [
        {
          "key": "age",
          "shape": [1],
          "dtype": "float32",
          "default_value": null,
          "normalizer_fn": null
        }
      ],
      "asserts": {
        "weak": ["返回NumericColumn对象", "key属性正确", "shape属性正确", "dtype属性正确"],
