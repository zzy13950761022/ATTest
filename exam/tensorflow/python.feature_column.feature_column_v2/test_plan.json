      "G1": "tests/test_tensorflow_python_feature_column_feature_column_v2_g1.py",
      "G2": "tests/test_tensorflow_python_feature_column_feature_column_v2_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心工厂函数",
      "entrypoints": ["numeric_column", "categorical_column_with_vocabulary_list"],
