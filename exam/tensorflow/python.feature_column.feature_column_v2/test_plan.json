      "oracle": "embedding_column函数规范",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 75,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": true
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "group_id": "G1",
      "name": "numeric_column错误处理",
      "priority": "High",
      "param_matrix": [
        {
          "key": "",
          "shape": [0],
          "dtype": "invalid",
          "default_value": "wrong_type",
          "normalizer_fn": "not_callable"
        }
      ],
      "asserts": {
        "weak": ["空key引发ValueError", "无效shape引发ValueError", "无效dtype引发TypeError", "类型不匹配引发TypeError"],
        "strong": ["错误消息格式", "嵌套错误处理", "边界条件组合"]
      },
      "oracle": "numeric_column错误规范",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 80,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": false
