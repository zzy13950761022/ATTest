      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "categorical_column_with_vocabulary_list基础创建",
      "priority": "High",
      "param_matrix": [
        {
          "key": "department",
          "vocabulary_list": ["math", "philosophy", "english"],
          "dtype": null,
          "default_value": -1,
          "num_oov_buckets": 0
        }
      ],
      "asserts": {
        "weak": ["返回CategoricalColumn对象", "key属性正确", "vocabulary_list属性正确", "default_value正确"],
