{
  "plan_version": 2,
  "target": "tensorflow.python.feature_column.feature_column_v2",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
