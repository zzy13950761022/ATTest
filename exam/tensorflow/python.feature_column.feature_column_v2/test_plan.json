      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "bucketized_column边界分桶",
      "priority": "High",
      "param_matrix": [
        {
          "source_column_key": "age",
          "boundaries": [18, 25, 30, 40, 50]
