      ],
      "asserts": {
        "weak": ["apply_function", "predicate_called", "dataset_method_called"],
        "strong": ["exact_predicate_passed", "method_args_correct"]
      },
      "oracle": "mock_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 4,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G1",
      "name": "predicate返回False时停止迭代",
      "priority": "High",
      "param_matrix": [
        {
          "predicate_type": "lambda_always_false",
          "dataset_type": "range",
          "dataset_args": {"stop": 10}
        }
      ],
      "asserts": {
        "weak": ["empty_result", "predicate_called_once"],
        "strong": ["exact_iteration_count", "no_extra_calls"]
