      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "函数返回类型验证",
      "priority": "High",
      "param_matrix": [
        {
          "predicate_type": "lambda_always_true",
          "dataset_type": "range",
          "dataset_args": {"stop": 10}
        }
      ],
      "asserts": {
        "weak": ["function_type", "callable", "deprecation_warning"],
        "strong": ["exact_signature", "warnings_count"]
      },
      "oracle": "function_inspection",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 4,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "转换函数正确包装predicate",
      "priority": "High",
      "param_matrix": [
        {
          "predicate_type": "lambda_less_than_5",
          "dataset_type": "range",
          "dataset_args": {"stop": 10}
