      "is_parametrized": false,
      "requires_mock": false
    },
    {
      "tc_id": "TC-08",
      "block_id": "CASE_08",
      "group_id": "G2",
      "name": "空数据集边界处理",
      "priority": "Medium",
      "param_matrix": [
        {
          "predicate_type": "lambda_always_true",
          "dataset_type": "empty",
          "dataset_args": {}
        }
      ],
      "asserts": {
        "weak": ["no_error", "empty_result"],
