{
  "plan_version": 2,
  "target": "tensorflow.python.data.experimental.ops.take_while_ops",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_data_experimental_ops_take_while_ops.py",
    "all_pattern": "tests/test_tensorflow_python_data_experimental_ops_take_while_ops_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_data_experimental_ops_take_while_ops_g1.py",
      "G2": "tests/test_tensorflow_python_data_experimental_ops_take_while_ops_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心函数功能验证",
      "entrypoints": ["take_while"],
      "smoke_set": ["CASE_01", "CASE_02", "CASE_03"],
      "deferred_set": ["CASE_04"],
      "note": "验证take_while函数基本功能和弃用警告"
    },
    {
      "group_id": "G2",
      "title": "边界与异常处理",
      "entrypoints": ["take_while"],
      "smoke_set": ["CASE_05"],
      "deferred_set": ["CASE_06", "CASE_07", "CASE_08"],
      "note": "测试异常输入和边界情况处理"
    }
  ],
  "cases": [
