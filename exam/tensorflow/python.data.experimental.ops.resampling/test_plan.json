{
  "plan_version": 2,
  "target": "tensorflow.python.data.experimental.ops.resampling",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_data_experimental_ops_resampling.py",
    "all_pattern": "tests/test_tensorflow_python_data_experimental_ops_resampling_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_data_experimental_ops_resampling_g1.py",
      "G2": "tests/test_tensorflow_python_data_experimental_ops_resampling_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心功能验证",
      "entrypoints": ["rejection_resample"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05"],
      "note": "验证基本功能和弃用警告"
    },
    {
      "group_id": "G2",
      "title": "分布调整与随机性",
      "entrypoints": ["rejection_resample"],
      "smoke_set": ["CASE_03"],
      "deferred_set": ["CASE_06", "CASE_07"],
      "note": "验证分布调整效果和种子控制"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "基本功能验证",
      "priority": "High",
      "param_matrix": [
        {
          "num_classes": 3,
          "dataset_size": 100,
          "seed": 42,
          "initial_dist": null
        }
      ],
      "asserts": {
        "weak": ["returns_function", "deprecation_warning", "dataset_type", "element_count"],
        "strong": ["distribution_convergence", "exact_seed_reproducibility", "class_func_calls"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "弃用警告验证",
      "priority": "High",
      "param_matrix": [
        {
          "num_classes": 2,
          "dataset_size": 50,
          "seed": null,
          "initial_dist": "provided"
        }
      ],
      "asserts": {
        "weak": ["deprecation_warning_triggered", "function_returned", "no_exceptions"],
        "strong": ["warning_message_contains", "warning_category", "stack_trace"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 4,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "分布调整验证",
      "priority": "High",
      "param_matrix": [
        {
          "num_classes": 4,
          "dataset_size": 200,
          "seed": 123,
          "initial_dist": null,
          "target_dist_type": "uniform"
        }
      ],
      "asserts": {
        "weak": ["output_distribution_close", "dataset_not_empty", "class_func_works"],
        "strong": ["distribution_error_bounds", "sampling_ratio_bounds", "statistical_significance"]
      },
      "oracle": "statistical_verification",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 85,
      "max_params": 6,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G1",
      "name": "可选参数initial_dist",
      "priority": "Medium",
      "param_matrix": [
        {
          "num_classes": 3,
          "dataset_size": 150,
          "seed": 42,
          "initial_dist": "provided"
        }
      ],
      "asserts": {
        "weak": ["function_works", "no_exceptions", "dataset_transformed"],
        "strong": ["initial_dist_used", "convergence_speed", "comparison_without_initial"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 75,
      "max_params": 5,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "group_id": "G1",
      "name": "边界值测试",
      "priority": "Medium",
      "param_matrix": [
        {
          "num_classes": 1,
          "dataset_size": 10,
          "seed": 0,
          "initial_dist": null
        }
      ],
      "asserts": {
        "weak": ["function_works", "no_exceptions", "basic_output"],
        "strong": ["edge_case_handling", "single_class_distribution", "small_dataset"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 4,
      "is_parametrized": false,
      "requires_mock": true
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "num_classes": 5,
        "dataset_size": 1000,
        "seed": 42,
        "initial_dist": null
      },
      "note": "扩展更多类别和大数据集"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {
        "num_classes": 4,
        "dataset_size": 200,
        "seed": 456,
        "initial_dist": null,
        "target_dist_type": "skewed"
      },
      "note": "扩展非均匀目标分布"
    },
    {
      "base_block_id": "CASE_04",
      "priority": "Low",
      "params": {
        "num_classes": 3,
        "dataset_size": 150,
        "seed": null,
        "initial_dist": "provided"
      },
      "note": "扩展无种子参数情况"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03"],
  "deferred_set": ["CASE_04", "CASE_05"]
}