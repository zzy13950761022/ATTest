{
  "plan_version": 2,
  "target": "tensorflow.lite.python.interpreter",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_lite_python_interpreter.py",
    "all_pattern": "tests/test_tensorflow_lite_python_interpreter_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_lite_python_interpreter_g1.py",
      "G2": "tests/test_tensorflow_lite_python_interpreter_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心Interpreter类功能",
      "entrypoints": ["Interpreter", "allocate_tensors", "get_input_details", "get_output_details", "set_tensor", "invoke", "get_tensor"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05", "CASE_06"],
      "note": "Interpreter类基本功能测试"
    },
    {
      "group_id": "G2",
      "title": "高级功能与异常处理",
      "entrypoints": ["SignatureRunner", "experimental_delegates", "num_threads", "experimental_preserve_all_tensors"],
      "smoke_set": ["CASE_03"],
      "deferred_set": ["CASE_07", "CASE_08"],
      "note": "签名运行器、委托、多线程等高级功能"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "模型加载与张量分配",
      "priority": "High",
      "param_matrix": [
        {
          "model_source": "model_path",
          "model_type": "simple_add",
          "num_threads": 1,
          "preserve_tensors": false
        }
      ],
      "asserts": {
        "weak": ["interpreter_created", "tensors_allocated", "input_details_valid", "output_details_valid"],
        "strong": ["tensor_indices_consistent", "quantization_params_valid"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "完整推理流程",
      "priority": "High",
      "param_matrix": [
        {
          "model_type": "simple_add",
          "input_shape": [2, 2],
          "dtype": "float32",
          "num_threads": 1
        }
      ],
      "asserts": {
        "weak": ["invoke_success", "output_shape_match", "output_dtype_match", "result_finite"],
        "strong": ["result_accuracy", "numerical_stability"]
      },
      "oracle": "numpy_computation",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 90,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "签名运行器基本功能",
      "priority": "High",
      "param_matrix": [
        {
          "model_type": "signed_model",
          "signature_key": "serving_default",
          "num_threads": 1
        }
      ],
      "asserts": {
        "weak": ["signature_runner_created", "signatures_listed", "inputs_outputs_accessible"],
        "strong": ["signature_invoke_accuracy", "multiple_signatures_supported"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 75,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": true
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G1",
      "name": "模型内容加载",
      "priority": "High",
      "param_matrix": [
        {
          "model_source": "model_content",
          "model_type": "simple_add",
          "num_threads": -1
        }
      ],
      "asserts": {
        "weak": ["interpreter_created", "tensors_allocated", "functionality_identical"],
        "strong": ["performance_comparable", "memory_usage_similar"]
      },
      "oracle": "model_path_comparison",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": true
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "model_source": "model_content",
        "model_type": "simple_add",
        "num_threads": 4,
        "preserve_tensors": true
      },
      "note": "模型内容加载+多线程+保留张量"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Medium",
      "params": {
        "model_type": "simple_add",
        "input_shape": [10, 10],
        "dtype": "float64",
        "num_threads": 2
      },
      "note": "更大输入+双精度+2线程"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Low",
      "params": {
        "model_type": "quantized_model",
        "input_shape": [1, 224, 224, 3],
        "dtype": "uint8",
        "num_threads": 1
      },
      "note": "量化模型测试"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03", "CASE_04"],
  "deferred_set": ["CASE_05", "CASE_06", "CASE_07", "CASE_08"]
}