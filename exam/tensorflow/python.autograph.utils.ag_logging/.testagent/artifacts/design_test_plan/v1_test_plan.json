    },
    {
      "group_id": "G3",
      "title": "调试跟踪函数族",
      "entrypoints": ["trace"],
      "smoke_set": ["CASE_04"],
      "deferred_set": ["CASE_08"],
      "note": "测试调试跟踪和交互模式输出"
    }
  ],
