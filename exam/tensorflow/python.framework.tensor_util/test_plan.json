{
  "plan_version": 2,
  "target": "tensorflow.python.framework.tensor_util",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_framework_tensor_util.py",
    "all_pattern": "tests/test_tensorflow_python_framework_tensor_util_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_framework_tensor_util_g1.py",
      "G2": "tests/test_tensorflow_python_framework_tensor_util_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心转换功能",
      "entrypoints": ["make_tensor_proto"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05", "CASE_06"],
      "note": "基本数据类型转换和形状处理"
    },
    {
      "group_id": "G2",
      "title": "边界与异常处理",
      "entrypoints": ["make_tensor_proto"],
      "smoke_set": ["CASE_03"],
      "deferred_set": ["CASE_07", "CASE_08"],
      "note": "错误场景和特殊数据类型"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "基本数据类型转换",
      "priority": "High",
      "param_matrix": [
        {
          "values": [1, 2, 3, 4],
          "dtype": "int32",
          "shape": null,
          "verify_shape": false,
          "allow_broadcast": false
        },
        {
          "values": [1.0, 2.0, 3.0],
          "dtype": "float32",
          "shape": null,
          "verify_shape": false,
          "allow_broadcast": false
        },
        {
          "values": [true, false, true],
          "dtype": "bool",
          "shape": null,
          "verify_shape": false,
          "allow_broadcast": false
        }
      ],
      "asserts": {
        "weak": ["proto_type", "dtype_match", "shape_match", "values_preserved"],
        "strong": ["exact_values", "serialization_roundtrip", "performance_baseline"]
      },
      "oracle": "numpy_array_comparison",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "形状验证与广播互斥",
      "priority": "High",
      "param_matrix": [
        {
          "values": [[1, 2], [3, 4]],
          "dtype": "int32",
          "shape": [2, 2],
          "verify_shape": true,
          "allow_broadcast": false
        },
        {
          "values": [[1, 2], [3, 4]],
          "dtype": "int32",
          "shape": [2, 3],
          "verify_shape": true,
          "allow_broadcast": false
        }
      ],
      "asserts": {
        "weak": ["shape_validation", "error_on_mismatch", "no_broadcast_when_disabled"],
        "strong": ["error_message_format", "exception_type_specific"]
      },
      "oracle": "shape_consistency_check",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "TensorProto输入直接返回",
      "priority": "High",
      "param_matrix": [
        {
          "values": "existing_tensor_proto",
          "dtype": null,
          "shape": null,
          "verify_shape": false,
          "allow_broadcast": false
        }
      ],
      "asserts": {
        "weak": ["same_object_returned", "no_modification", "identity_check"],
        "strong": ["memory_address_match", "deep_equality"]
      },
      "oracle": "object_identity",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 3,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G1",
      "name": "特殊数据类型支持",
      "priority": "High",
      "param_matrix": [
        {
          "values": "float16_array",
          "dtype": "float16",
          "shape": null,
          "verify_shape": false,
          "allow_broadcast": false
        },
        {
          "values": "bfloat16_array",
          "dtype": "bfloat16",
          "shape": null,
          "verify_shape": false,
          "allow_broadcast": false
        }
      ],
      "asserts": {
        "weak": ["dtype_supported", "values_approximate", "no_crash"],
        "strong": ["precision_preserved", "roundtrip_accuracy"]
      },
      "oracle": "special_dtype_handling",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 75,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "values": "numpy_ndarray",
        "dtype": "float64",
        "shape": [3, 3],
        "verify_shape": false,
        "allow_broadcast": false
      },
      "note": "numpy数组输入扩展"
    },
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "values": "string_array",
        "dtype": "string",
        "shape": [2, 2],
        "verify_shape": false,
        "allow_broadcast": false
      },
      "note": "字符串数据类型扩展"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Medium",
      "params": {
        "values": "scalar_value",
