{
  "plan_version": 2,
  "target": "tensorflow.python.training.input",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_training_input.py",
    "all_pattern": "tests/test_tensorflow_python_training_input.py"
  },
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "name": "基本批量处理功能验证",
      "priority": "High",
      "param_matrix": [
        {
          "function": "batch",
          "tensors": "simple_tensor_list",
          "batch_size": 2,
          "capacity": 32,
          "shuffle": false,
          "enqueue_many": false,
          "dynamic_pad": false,
          "allow_smaller_final_batch": false
        }
      ],
      "asserts": {
        "weak": ["deprecation_warning", "output_shape", "batch_size_correct", "queue_runner_added"],
        "strong": ["exact_values_match", "no_data_loss", "deterministic_order"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 80,
      "max_params": 8,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": [
        "tensorflow.python.training.queue_runner.QueueRunner",
        "tensorflow.python.ops.data_flow_ops.FIFOQueue",
        "tensorflow.python.training.queue_runner.add_queue_runner",
        "warnings.warn"
      ]
    },
