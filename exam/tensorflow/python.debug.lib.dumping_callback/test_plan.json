{
  "plan_version": 2,
  "target": "tensorflow.python.debug.lib.dumping_callback",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_debug_lib_dumping_callback.py",
    "all_pattern": "tests/test_tensorflow_python_debug_lib_dumping_callback_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_debug_lib_dumping_callback_g1.py",
      "G2": "tests/test_tensorflow_python_debug_lib_dumping_callback_g2.py",
      "G3": "tests/test_tensorflow_python_debug_lib_dumping_callback_g3.py"
    }
  },
  "active_group_order": ["G1", "G2", "G3"],
  "groups": [
