      "deferred_set": ["CASE_06", "CASE_07", "CASE_08"],
      "note": "测试不同tensor_debug_mode的张量信息提取"
    },
    {
      "group_id": "G3",
      "title": "过滤与缓冲区功能",
      "entrypoints": ["enable_dump_debug_info"],
      "smoke_set": ["CASE_09"],
      "deferred_set": ["CASE_10", "CASE_11"],
      "note": "测试op_regex、tensor_dtypes过滤和环形缓冲区"
    }
