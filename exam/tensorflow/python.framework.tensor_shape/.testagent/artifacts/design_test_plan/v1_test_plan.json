{
  "plan_version": 2,
  "target": "tensorflow.python.framework.tensor_shape",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_framework_tensor_shape.py",
    "all_pattern": "tests/test_tensorflow_python_framework_tensor_shape_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_framework_tensor_shape_dimension.py",
      "G2": "tests/test_tensorflow_python_framework_tensor_shape_tensorshape.py",
      "G3": "tests/test_tensorflow_python_framework_tensor_shape_helpers.py"
    }
  },
  "active_group_order": ["G1", "G2", "G3"],
  "groups": [
    {
      "group_id": "G1",
      "title": "Dimension 类测试",
      "entrypoints": ["Dimension", "__init__", "value", "__eq__", "__add__", "__mul__"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_06", "CASE_07"],
      "note": "测试 Dimension 类的基本构造、属性和运算"
    },
    {
      "group_id": "G2",
      "title": "TensorShape 类测试",
      "entrypoints": ["TensorShape", "__init__", "rank", "dims", "is_compatible_with", "__iter__"],
      "smoke_set": ["CASE_03", "CASE_04"],
      "deferred_set": ["CASE_08", "CASE_09"],
      "note": "测试 TensorShape 类的构造、属性和形状操作"
    },
    {
      "group_id": "G3",
      "title": "辅助函数与兼容性测试",
      "entrypoints": ["dimension_value", "as_dimension", "as_shape", "unknown_shape", "enable_v2_tensorshape", "disable_v2_tensorshape"],
      "smoke_set": ["CASE_05"],
      "deferred_set": ["CASE_10"],
      "note": "测试辅助函数和 V1/V2 兼容性切换"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "Dimension 基本构造与属性",
      "priority": "High",
      "param_matrix": [
        {"value": 5, "expected_value": 5},
        {"value": 0, "expected_value": 0},
        {"value": None, "expected_value": null}
      ],
      "asserts": {
        "weak": ["value", "is_known", "equality"],
        "strong": ["hash", "repr", "str"]
      },
      "oracle": "Dimension 构造函数",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "Dimension 算术运算",
      "priority": "High",
      "param_matrix": [
        {"value1": 3, "value2": 4, "op": "add", "expected": 7},
        {"value1": 3, "value2": 4, "op": "mul", "expected": 12},
        {"value1": None, "value2": 5, "op": "add", "expected": null}
      ],
      "asserts": {
        "weak": ["operation_result", "type_check"],
        "strong": ["commutative", "associative", "distributive"]
      },
      "oracle": "Dimension 运算符重载",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "TensorShape 基本构造",
      "priority": "High",
      "param_matrix": [
        {"dims": [2, 3], "expected_rank": 2, "expected_dims": [2, 3]},
        {"dims": [], "expected_rank": 0, "expected_dims": []},
        {"dims": null, "expected_rank": null, "expected_dims": null}
      ],
      "asserts": {
        "weak": ["rank", "dims", "is_fully_defined"],
        "strong": ["repr", "str", "len"]
      },
      "oracle": "TensorShape 构造函数",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G2",
      "name": "形状兼容性检查",
      "priority": "High",
      "param_matrix": [
        {"shape1": [2, 3], "shape2": [2, 3], "expected": true},
        {"shape1": [2, null], "shape2": [2, 5], "expected": true},
        {"shape1": [2, 3], "shape2": [3, 2], "expected": false}
      ],
      "asserts": {
        "weak": ["compatibility_result", "symmetry"],
        "strong": ["transitive", "edge_cases"]
      },
      "oracle": "is_compatible_with 方法",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "group_id": "G3",
      "name": "辅助函数基本功能",
      "priority": "High",
      "param_matrix": [
        {"func": "dimension_value", "input": 5, "expected": 5},
        {"func": "as_dimension", "input": 3, "expected_type": "Dimension"},
        {"func": "as_shape", "input": [2, 3], "expected_type": "TensorShape"}
      ],
      "asserts": {
        "weak": ["return_value", "type_check"],
        "strong": ["idempotent", "edge_handling"]
      },
      "oracle": "辅助函数文档",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 75,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {"value": -1, "expected_error": "ValueError"},
      "note": "负维度值异常测试"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Medium",
      "params": {"value1": 10, "value2": 20, "op": "sub", "expected_error": "TypeError"},
      "note": "不支持的运算异常测试"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {"dims": [2, null, 3], "expected_rank": 3, "expected_dims": [2, null, 3]},
      "note": "混合已知/未知维度形状"
    },
    {
      "base_block_id": "CASE_04",
      "priority": "Medium",
      "params": {"shape1": [null, null], "shape2": [null, null], "expected": true},
      "note": "完全未知形状兼容性"
    },
    {
      "base_block_id": "CASE_05",
      "priority": "Medium",
      "params": {"func": "unknown_shape", "input": 3, "expected_rank": 3},
      "note": "unknown_shape 函数测试"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03", "CASE_04", "CASE_05"],
  "deferred_set": ["CASE_06", "CASE_07", "CASE_08", "CASE_09", "CASE_10"]
}