      "param_matrix": [
        {"x_shape": [2, 3], "dtype": "float32", "axis": -1, "epsilon": 1e-12}
      ],
      "asserts": {
        "weak": ["shape", "dtype", "finite", "norm_close_to_one"],
        "strong": ["numerical_accuracy", "gradient_check", "broadcast_correctness"]
      },
      "oracle": "手动计算L2范数",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 80,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "name": "swish激活函数数值正确性",
      "priority": "High",
      "param_matrix": [
        {"features_shape": [3, 4], "dtype": "float32", "beta": 1.0}
