{
  "plan_version": 2,
  "target": "tensorflow.python.framework.test_util",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_framework_test_util.py",
    "all_pattern": "tests/test_tensorflow_python_framework_test_util_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_framework_test_util_g1.py",
      "G2": "tests/test_tensorflow_python_framework_test_util_g2.py",
      "G3": "tests/test_tensorflow_python_framework_test_util_g3.py"
    }
  },
  "active_group_order": ["G1", "G2", "G3"],
  "groups": [
    {
      "group_id": "G1",
      "title": "测试基类与装饰器",
      "entrypoints": ["TensorFlowTestCase", "run_in_graph_and_eager_modes"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_06", "CASE_07"],
      "note": "测试基类继承和模式切换装饰器"
    },
    {
      "group_id": "G2",
      "title": "图形比较与断言函数",
      "entrypoints": ["assert_equal_graph_def", "assertAllClose"],
      "smoke_set": ["CASE_03"],
      "deferred_set": ["CASE_08", "CASE_09"],
      "note": "图形协议缓冲区比较和数值断言"
    },
    {
      "group_id": "G3",
      "title": "设备管理与测试环境",
      "entrypoints": ["gpu_device_name", "create_local_cluster"],
      "smoke_set": ["CASE_04", "CASE_05"],
      "deferred_set": ["CASE_10", "CASE_11"],
      "note": "设备检测和分布式测试环境"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "TensorFlowTestCase 基类继承",
      "priority": "High",
      "param_matrix": [
        {
          "test_method": "test_addition",
          "mode": "graph",
          "use_eager": false
        }
      ],
      "asserts": {
        "weak": ["inheritance", "method_availability", "basic_assertion"],
        "strong": ["full_assertion_suite", "session_management", "state_isolation"]
      },
      "oracle": "unittest.TestCase",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "run_in_graph_and_eager_modes 装饰器",
      "priority": "High",
      "param_matrix": [
        {
          "test_method": "test_constant",
          "mode": "both",
          "use_mock": false
        }
      ],
      "asserts": {
        "weak": ["decorator_applies", "mode_switching", "test_execution"],
        "strong": ["mode_isolation", "state_cleanup", "error_propagation"]
      },
      "oracle": "function_wrapping",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 75,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "assert_equal_graph_def 基本比较",
      "priority": "High",
      "param_matrix": [
        {
          "graph_type": "simple",
          "node_count": 2,
          "check_version": true
        }
      ],
      "asserts": {
        "weak": ["graph_equality", "no_exception", "basic_structure"],
        "strong": ["node_attribute_match", "version_check", "tolerance_handling"]
      },
      "oracle": "graph_protobuf_equality",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 85,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": true
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G3",
      "name": "gpu_device_name 设备检测",
      "priority": "High",
      "param_matrix": [
        {
          "device_present": true,
          "use_mock": true,
          "fallback_cpu": true
        }
      ],
      "asserts": {
        "weak": ["returns_string_or_none", "no_crash", "basic_format"],
        "strong": ["device_name_format", "cuda_detection", "fallback_behavior"]
      },
      "oracle": "device_detection",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": true
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "group_id": "G3",
      "name": "create_local_cluster 基本创建",
      "priority": "High",
      "param_matrix": [
        {
          "worker_count": 2,
          "ps_count": 1,
          "use_mock": true
        }
      ],
      "asserts": {
        "weak": ["cluster_created", "no_exception", "basic_structure"],
        "strong": ["worker_ps_separation", "port_allocation", "cleanup_handling"]
      },
      "oracle": "distributed_cluster_creation",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 90,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "test_method": "test_multiplication",
        "mode": "eager",
        "use_eager": true
      },
      "note": "作为参数扩展：测试 eager 模式"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Medium",
      "params": {
        "test_method": "test_variable",
        "mode": "graph",
        "use_mock": true
      },
      "note": "作为参数扩展：测试 graph 模式单独"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {
        "graph_type": "complex",
        "node_count": 5,
        "check_version": false
      },
      "note": "作为参数扩展：复杂图形比较"
    },
    {
      "base_block_id": "CASE_04",
      "priority": "Medium",
      "params": {
        "device_present": false,
        "use_mock": true,
        "fallback_cpu": false
      },
      "note": "作为参数扩展：无 GPU 设备情况"
    },
    {
      "base_block_id": "CASE_05",
      "priority": "Medium",
      "params": {
        "worker_count": 1,
        "ps_count": 0,
        "use_mock": true
      },
      "note": "作为参数扩展：最小集群配置"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03", "CASE_04", "CASE_05"],
  "deferred_set": ["CASE_06", "CASE_07", "CASE_08", "CASE_09", "CASE_10", "CASE_11"]
}