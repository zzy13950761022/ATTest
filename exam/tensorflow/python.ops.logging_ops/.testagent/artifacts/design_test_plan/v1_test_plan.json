{
  "plan_version": 2,
  "target": "tensorflow.python.ops.logging_ops",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {"include": "SMOKE_SET", "assert_level": "weak", "max_blocks": 5},
    "roundN": {"only_fix_failed_blocks": true, "block_limit": 3, "promote_deferred": true},
    "final": {"enable_strong_asserts": true, "coverage_optional": true}
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_ops_logging_ops.py",
    "all_pattern": "tests/test_tensorflow_python_ops_logging_ops.py"
  },
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "name": "print_v2基础打印功能验证",
      "priority": "High",
      "param_matrix": [
        {
          "function": "print_v2",
          "inputs": ["test_tensor"],
          "output_stream": "stdout",
          "summarize": 3,
          "sep": " ",
          "end": "\\n"
        }
      ],
      "asserts": {
        "weak": ["output_captured", "no_exception", "function_executed"],
        "strong": ["exact_output_match", "performance_bounds", "memory_usage"]
      },
      "oracle": "sys.stdout内容验证",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": ["sys.stdout", "sys.stderr"]
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "name": "图像摘要4-D张量处理",
      "priority": "High",
      "param_matrix": [
        {
          "function": "image_summary",
          "tag": "test_image",
          "tensor_shape": [2, 32, 32, 3],
          "max_images": 3,
          "channels": 3,
          "value_range": [0, 255]
        }
      ],
      "asserts": {
        "weak": ["returns_tensor", "tensor_type_string", "no_exception", "shape_correct"],
        "strong": ["summary_proto_valid", "image_count_correct", "value_range_check"]
      },
      "oracle": "返回字符串张量验证",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 75,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "name": "音频摘要值范围检查和采样率验证",
      "priority": "High",
      "param_matrix": [
        {
          "function": "audio_summary",
          "tag": "test_audio",
          "tensor_shape": [2, 16000],
          "sample_rate": 16000.0,
          "max_outputs": 3,
          "value_range": [-0.5, 0.5]
        }
      ],
      "asserts": {
        "weak": ["returns_tensor", "no_exception", "sample_rate_accepted", "tensor_type_string"],
        "strong": ["audio_proto_valid", "value_range_enforced", "max_outputs_respected"]
      },
      "oracle": "音频摘要协议缓冲区验证",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 75,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "name": "标量摘要单/多标签支持",
      "priority": "High",
      "param_matrix": [
        {
          "function": "scalar_summary",
          "tags": "single_tag",
          "values": "scalar_tensor",
          "test_type": "single"
        },
        {
          "function": "scalar_summary",
          "tags": ["tag1", "tag2", "tag3"],
          "values": ["val1", "val2", "val3"],
          "test_type": "multiple"
        }
      ],
      "asserts": {
        "weak": ["returns_tensor", "no_exception", "tensor_type_string", "list_length_match"],
        "strong": ["scalar_values_correct", "tag_mapping_accurate", "collection_added"]
      },
      "oracle": "标量摘要标签值映射验证",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 80,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "name": "输出流切换功能测试",
      "priority": "High",
      "param_matrix": [
        {
          "function": "print_v2",
          "inputs": ["stream_test"],
          "output_stream": "stderr",
          "test_stream": "stderr"
        },
        {
          "function": "print_v2",
          "inputs": ["log_test"],
          "output_stream": "log:info",
          "test_stream": "log_info"
        }
      ],
      "asserts": {
        "weak": ["output_captured", "stream_correct", "no_exception", "function_executed"],
        "strong": ["log_level_correct", "exact_message_match", "no_cross_stream_leak"]
      },
      "oracle": "不同输出流内容验证",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": ["sys.stderr", "logging.getLogger"]
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "function": "print_v2",
        "inputs": ["tensor1", "tensor2", "python_obj"],
        "output_stream": "stdout",
        "summarize": 5,
        "sep": ", ",
        "end": "\\n---\\n"
      },
      "note": "多输入和自定义分隔符扩展"
    },
    {
