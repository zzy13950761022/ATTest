{
  "plan_version": 2,
  "target": "tensorflow.python.compiler.xla.jit",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_compiler_xla_jit.py",
    "all_pattern": "tests/test_tensorflow_python_compiler_xla_jit_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_compiler_xla_jit_g1.py",
      "G2": "tests/test_tensorflow_python_compiler_xla_jit_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心上下文管理器功能",
      "entrypoints": ["experimental_jit_scope"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05"],
      "note": "测试基本上下文管理器功能和异常场景"
    },
    {
      "group_id": "G2",
      "title": "参数功能与高级用法",
      "entrypoints": ["experimental_jit_scope"],
      "smoke_set": ["CASE_03", "CASE_04"],
      "deferred_set": ["CASE_06", "CASE_07"],
      "note": "测试compile_ops参数和separate_compiled_gradients功能"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "基本上下文管理器功能验证",
      "priority": "High",
      "param_matrix": [
        {
          "compile_ops": true,
          "separate_compiled_gradients": false,
          "execution_mode": "graph"
        }
      ],
      "asserts": {
        "weak": ["context_manager_created", "no_exception", "scope_entered"],
        "strong": ["graph_attr_set", "state_restored", "counter_updated"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 3,
      "is_parametrized": false,
      "requires_mock": false
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "eager_execution模式异常抛出",
      "priority": "High",
      "param_matrix": [
        {
          "compile_ops": true,
          "separate_compiled_gradients": false,
          "execution_mode": "eager"
        }
      ],
      "asserts": {
        "weak": ["runtime_error_raised", "error_message_contains_eager"],
        "strong": ["error_type_exact", "no_side_effects"]
      },
      "oracle": "RuntimeError",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 50,
      "max_params": 3,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "compile_ops布尔参数功能测试",
      "priority": "High",
      "param_matrix": [
        {
          "compile_ops": false,
          "separate_compiled_gradients": false,
          "execution_mode": "graph"
        }
      ],
      "asserts": {
        "weak": ["context_manager_created", "no_exception", "compile_disabled"],
        "strong": ["attr_value_correct", "nested_scope_works"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 3,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G2",
      "name": "compile_ops可调用参数条件编译",
      "priority": "High",
      "param_matrix": [
        {
          "compile_ops": "callable_true",
          "separate_compiled_gradients": false,
          "execution_mode": "graph"
        }
      ],
      "asserts": {
        "weak": ["context_manager_created", "callable_accepted", "no_exception"],
        "strong": ["callable_invoked", "conditional_compilation_works"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 80,
      "max_params": 3,
      "is_parametrized": true,
      "requires_mock": false
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "compile_ops": true,
        "separate_compiled_gradients": true,
        "execution_mode": "graph"
      },
      "note": "测试separate_compiled_gradients参数扩展"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {
        "compile_ops": true,
        "separate_compiled_gradients": false,
        "execution_mode": "graph"
      },
      "note": "测试compile_ops=True参数扩展"
    },
    {
      "base_block_id": "CASE_04",
      "priority": "Medium",
      "params": {
        "compile_ops": "callable_false",
        "separate_compiled_gradients": false,
        "execution_mode": "graph"
      },
      "note": "测试返回False的可调用参数"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03", "CASE_04"],
  "deferred_set": ["CASE_05", "CASE_06", "CASE_07"]
}