{
  "plan_version": 2,
  "target": "tensorflow.python.compiler.xla.xla",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_compiler_xla_xla.py",
    "all_pattern": "tests/test_tensorflow_python_compiler_xla_xla_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_compiler_xla_xla_core.py",
      "G2": "tests/test_tensorflow_python_compiler_xla_xla_special.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心编译功能",
      "entrypoints": ["compile", "_compile_internal"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05", "CASE_06"],
      "note": "测试 compile() 函数的基本编译功能"
    },
    {
      "group_id": "G2",
      "title": "特殊场景与边界处理",
      "entrypoints": ["compile"],
      "smoke_set": ["CASE_03"],
      "deferred_set": ["CASE_07", "CASE_08"],
      "note": "测试特殊输出、边界值和异常场景"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "基本编译功能验证",
      "priority": "High",
      "param_matrix": [
        {
          "computation_type": "simple_arithmetic",
          "inputs_type": "tensor_list",
          "dtype": "float32",
          "shape": [2, 3],
          "device": "cpu"
        }
      ],
      "asserts": {
        "weak": ["output_shape", "output_dtype", "finite_check", "basic_equality"],
        "strong": ["numerical_equality", "gradient_check", "execution_order"]
      },
      "oracle": "direct_computation_call",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "None与空输入处理",
      "priority": "High",
      "param_matrix": [
        {
          "computation_type": "identity",
          "inputs_type": "none",
          "dtype": "float32",
          "shape": [1],
          "device": "cpu"
        },
        {
          "computation_type": "identity",
          "inputs_type": "empty_list",
          "dtype": "float32",
          "shape": [1],
          "device": "cpu"
        }
      ],
      "asserts": {
        "weak": ["output_consistency", "no_error"],
        "strong": ["behavior_equivalence", "state_consistency"]
      },
      "oracle": "direct_computation_call",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "单值输出包装测试",
      "priority": "High",
      "param_matrix": [
        {
          "computation_type": "single_value",
          "inputs_type": "tensor_list",
          "dtype": "float32",
          "shape": [3],
          "device": "cpu"
        }
      ],
      "asserts": {
        "weak": ["output_type", "tuple_wrapping", "value_equality"],
        "strong": ["type_consistency", "gradient_preservation"]
      },
      "oracle": "direct_computation_call",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G1",
      "name": "嵌套输入结构转换",
      "priority": "High",
      "param_matrix": [
        {
          "computation_type": "nested_processing",
          "inputs_type": "nested_structure",
          "dtype": "float32",
          "shape": [2, 2],
          "device": "cpu"
        }
      ],
      "asserts": {
        "weak": ["structure_preservation", "value_equality", "no_error"],
        "strong": ["gradient_flow", "execution_semantics"]
      },
      "oracle": "direct_computation_call",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 80,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": false
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "computation_type": "simple_arithmetic",
        "inputs_type": "tensor_list",
        "dtype": "float64",
        "shape": [5, 5],
        "device": "cpu"
      },
      "note": "扩展数据类型和形状"
    },
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "computation_type": "matrix_operation",
        "inputs_type": "tensor_list",
        "dtype": "float32",
        "shape": [10, 10],
        "device": "cpu"
      },
      "note": "扩展计算复杂度"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Medium",
      "params": {
        "computation_type": "multi_input",
        "inputs_type": "none",
        "dtype": "float32",
        "shape": [2, 2],
        "device": "cpu"
      },
      "note": "多输入函数的None输入处理"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {
        "computation_type": "single_value",
        "inputs_type": "tensor_list",
        "dtype": "int32",
        "shape": [4],
        "device": "cpu"
      },
      "note": "整数类型的单值输出"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03"],
  "deferred_set": ["CASE_04", "CASE_05", "CASE_06", "CASE_07", "CASE_08"]
}