{
  "plan_version": 2,
  "target": "tensorflow.python.framework.graph_io",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_framework_graph_io.py",
    "all_pattern": "tests/test_tensorflow_python_framework_graph_io_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_framework_graph_io_g1.py",
      "G2": "tests/test_tensorflow_python_framework_graph_io_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心写入功能测试",
      "entrypoints": ["write_graph"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05"],
      "note": "测试 Graph 和 GraphDef 对象的基本写入功能"
    },
    {
      "group_id": "G2",
      "title": "异常与边界测试",
      "entrypoints": ["write_graph"],
      "smoke_set": ["CASE_03"],
      "deferred_set": ["CASE_06", "CASE_07"],
      "note": "测试无效输入、边界条件和错误处理"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "Graph对象写入文本格式文件",
      "priority": "High",
      "param_matrix": [
        {
          "input_type": "Graph",
          "as_text": true,
          "logdir": "/tmp/test_dir",
          "name": "graph.pbtxt",
          "graph_size": "small"
        }
      ],
      "asserts": {
        "weak": ["file_exists", "path_returned", "file_not_empty", "basic_content"],
        "strong": ["content_parsable", "graph_structure_preserved", "metadata_complete"]
      },
      "oracle": "tf.io.write_graph",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "GraphDef对象写入二进制格式文件",
      "priority": "High",
      "param_matrix": [
        {
          "input_type": "GraphDef",
          "as_text": false,
          "logdir": "/tmp/test_dir",
          "name": "graph.pb",
          "graph_size": "small"
        }
      ],
      "asserts": {
        "weak": ["file_exists", "path_returned", "file_not_empty", "binary_format"],
        "strong": ["binary_parsable", "graph_structure_preserved", "binary_consistency"]
      },
      "oracle": "tf.io.write_graph",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "无效输入类型异常触发",
      "priority": "High",
      "param_matrix": [
        {
          "input_type": "invalid_string",
          "as_text": true,
          "logdir": "/tmp/test_dir",
          "name": "test.pbtxt",
          "expected_error": "TypeError"
        },
        {
          "input_type": "None",
          "as_text": true,
          "logdir": "/tmp/test_dir",
          "name": "test.pbtxt",
          "expected_error": "TypeError"
        }
      ],
      "asserts": {
        "weak": ["exception_raised", "exception_type", "error_message_contains"],
        "strong": ["exception_context", "stack_trace_clean", "no_side_effects"]
      },
      "oracle": "tf.io.write_graph",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G1",
      "name": "本地目录自动创建功能",
      "priority": "High",
      "param_matrix": [
        {
          "input_type": "Graph",
          "as_text": true,
          "logdir": "/tmp/nonexistent_dir",
          "name": "test.pbtxt",
          "graph_size": "small"
        }
      ],
      "asserts": {
        "weak": ["directory_created", "file_exists", "path_returned"],
        "strong": ["directory_permissions", "cleanup_possible", "no_race_condition"]
      },
      "oracle": "tf.io.write_graph",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 5,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "group_id": "G1",
      "name": "GCS远程路径支持",
      "priority": "Medium",
      "param_matrix": [
        {
          "input_type": "Graph",
          "as_text": true,
          "logdir": "gs://test-bucket/path",
          "name": "test.pbtxt",
          "graph_size": "small"
        }
      ],
      "asserts": {
        "weak": ["path_returned", "no_local_directory_creation"],
        "strong": ["gcs_client_called", "gcs_path_handled", "network_error_handled"]
      },
      "oracle": "tf.io.write_graph",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 80,
      "max_params": 5,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-06",
      "block_id": "CASE_06",
      "group_id": "G2",
      "name": "空图和特殊字符处理",
      "priority": "Medium",
      "param_matrix": [
        {
          "input_type": "Graph",
          "as_text": true,
          "logdir": "/tmp/test_dir",
          "name": "test_特殊@字符.pbtxt",
          "graph_size": "empty"
        }
      ],
      "asserts": {
        "weak": ["file_exists", "path_returned", "special_chars_handled"],
        "strong": ["filename_encoding", "cross_platform_compat", "unicode_support"]
      },
      "oracle": "tf.io.write_graph",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 5,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-07",
      "block_id": "CASE_07",
      "group_id": "G2",
      "name": "文件权限错误处理",
      "priority": "Low",
      "param_matrix": [
        {
          "input_type": "Graph",
          "as_text": true,
          "logdir": "/root/protected",
          "name": "test.pbtxt",
          "graph_size": "small",
          "expected_error": "PermissionError"
        }
      ],
      "asserts": {
        "weak": ["exception_raised", "exception_type", "clean_error"],
        "strong": ["resource_cleanup", "no_partial_write", "error_propagation"]
      },
      "oracle": "tf.io.write_graph",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 6,
      "is_parametrized": false,
      "requires_mock": true
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "input_type": "Graph",
        "as_text": true,
        "logdir": "/tmp/test_dir",
        "name": "graph.pbtxt",
        "graph_size": "large"
      },
      "note": "大图对象序列化性能测试"
    },
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "input_type": "GraphDef",
        "as_text": true,
        "logdir": "/tmp/test_dir",
        "name": "graph.pbtxt",
        "graph_size": "small"
      },
      "note": "GraphDef文本格式写入"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {
        "input_type": "empty_string",
        "as_text": true,
        "logdir": "/tmp/test_dir",
        "name": "test.pbtxt",
        "expected_error": "TypeError"
      },
      "note": "空字符串输入异常"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03", "CASE_04"],
  "deferred_set": ["CASE_05", "CASE_06", "CASE_07"]
}