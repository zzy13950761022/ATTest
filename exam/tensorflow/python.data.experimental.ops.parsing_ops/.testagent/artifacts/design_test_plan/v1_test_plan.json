{
  "plan_version": 2,
  "target": "tensorflow.python.data.experimental.ops.parsing_ops",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_data_experimental_ops_parsing_ops.py",
    "all_pattern": "tests/test_tensorflow_python_data_experimental_ops_parsing_ops_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_data_experimental_ops_parsing_ops_g1.py",
      "G2": "tests/test_tensorflow_python_data_experimental_ops_parsing_ops_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心功能与参数验证",
      "entrypoints": ["parse_example_dataset"],
      "smoke_set": ["CASE_01", "CASE_02", "CASE_03"],
      "deferred_set": ["CASE_04"],
      "note": "基本功能测试和参数验证"
    },
    {
      "group_id": "G2",
      "title": "特征类型与边界测试",
      "entrypoints": ["parse_example_dataset"],
      "smoke_set": ["CASE_05"],
      "deferred_set": ["CASE_06", "CASE_07", "CASE_08"],
      "note": "多种特征类型和边界条件测试"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "FixedLenFeature基本解析",
      "priority": "High",
      "param_matrix": [
        {
          "feature_type": "FixedLenFeature",
          "dtype": "float32",
          "shape": [],
          "num_parallel_calls": 1,
          "deterministic": null,
          "dataset_size": 10
        }
      ],
      "asserts": {
        "weak": ["returns_function", "output_shape", "output_dtype", "dataset_size"],
        "strong": ["exact_values", "deterministic_behavior", "performance_baseline"]
      },
      "oracle": "tf.io.parse_example",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": false,
      "requires_mock": false
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "features参数验证",
      "priority": "High",
      "param_matrix": [
        {
          "features": null,
          "expected_error": "ValueError"
        },
        {
          "features": {},
          "expected_error": "ValueError"
        }
      ],
      "asserts": {
        "weak": ["raises_error", "error_type"],
        "strong": ["error_message", "stack_trace"]
      },
      "oracle": "none",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 50,
      "max_params": 3,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G1",
      "name": "并行解析功能",
      "priority": "High",
      "param_matrix": [
        {
          "feature_type": "FixedLenFeature",
          "dtype": "int32",
          "shape": [3],
          "num_parallel_calls": 4,
          "deterministic": true,
          "dataset_size": 20
        }
      ],
      "asserts": {
        "weak": ["returns_function", "output_shape", "output_dtype", "parallel_execution"],
        "strong": ["performance_improvement", "deterministic_order"]
      },
      "oracle": "tf.io.parse_example",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 80,
      "max_params": 6,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G1",
      "name": "num_parallel_calls边界值",
      "priority": "Medium",
      "param_matrix": [
        {
          "num_parallel_calls": 0,
          "expected_error": "ValueError"
        },
        {
          "num_parallel_calls": -1,
          "expected_error": "ValueError"
        }
      ],
      "asserts": {
        "weak": ["raises_error", "error_type"],
        "strong": ["error_message", "input_validation"]
      },
      "oracle": "none",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 50,
      "max_params": 3,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "group_id": "G2",
      "name": "多种特征类型支持",
      "priority": "High",
      "param_matrix": [
        {
          "feature_types": ["FixedLenFeature", "VarLenFeature"],
          "dtype_combinations": ["float32", "int64"],
          "deterministic": false,
          "dataset_size": 15
        }
      ],
      "asserts": {
        "weak": ["returns_function", "output_structure", "feature_count", "dataset_size"],
        "strong": ["exact_values_all_features", "sparse_tensor_handling", "ragged_tensor_handling"]
      },
      "oracle": "tf.io.parse_example",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 90,
      "max_params": 7,
      "is_parametrized": false,
      "requires_mock": true
    },
    {
      "tc_id": "TC-06",
      "block_id": "CASE_06",
      "group_id": "G2",
      "name": "deterministic参数行为",
      "priority": "Medium",
      "param_matrix": [
        {
          "deterministic": null,
          "feature_type": "FixedLenFeature",
          "dtype": "float64",
          "dataset_size": 8
        },
        {
          "deterministic": true,
          "feature_type": "FixedLenFeature",
          "dtype": "float64",
          "dataset_size": 8
        },
        {
          "deterministic": false,
          "feature_type": "FixedLenFeature",
          "dtype": "float64",
          "dataset_size": 8
        }
      ],
      "asserts": {
        "weak": ["returns_function", "output_consistency", "dataset_size"],
        "strong": ["order_preservation", "performance_difference", "dataset_option_interaction"]
      },
      "oracle": "none",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 85,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": true
    },
    {
      "tc_id": "TC-07",
      "block_id": "CASE_07",
      "group_id": "G2",
      "name": "空数据集处理",
      "priority": "Low",
      "param_matrix": [
        {
          "feature_type": "FixedLenFeature",
          "dtype": "int32",
          "dataset_size": 0,
          "deterministic": true
        }
      ],
      "asserts": {
        "weak": ["returns_function", "empty_dataset", "output_structure"],
        "strong": ["memory_usage", "edge_case_handling", "performance_empty"]
      },
      "oracle": "none",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 4,
      "is_parametrized": false,
      "requires_mock": false
    },
    {
      "tc_id": "TC-08",
      "block_id": "CASE_08",
      "group_id": "G2",
      "name": "无效输入数据集验证",
      "priority": "Medium",
      "param_matrix": [
        {
          "input_type": "non_string_dataset",
          "expected_error": "InvalidArgumentError"
        },
        {
          "input_type": "wrong_shape_dataset",
          "expected_error": "InvalidArgumentError"
        }
      ],
      "asserts": {
        "weak": ["raises_error", "error_type", "input_validation"],
        "strong": ["error_message_detail", "element_spec_validation"]
      },
      "oracle": "none",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 75,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "feature_type": "FixedLenFeature",
        "dtype": "float64",
        "shape": [2, 3],
        "num_parallel_calls": 2,
        "deterministic": true,
        "dataset_size": 5
      },
      "note": "扩展数据类型和形状"
    },
    {
      "base_block_id": "CASE_01",
      "priority": "Low",
      "params": {
        "feature_type": "FixedLenFeature",
        "dtype": "string",
        "shape": [],
        "num_parallel_calls": 1,
        "deterministic": null,
        "dataset_size": 3
      },
      "note": "扩展字符串数据类型"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {
        "feature_type": "FixedLenFeature",
        "dtype": "int64",
        "shape": [],
        "num_parallel_calls": 8,
        "deterministic": false,
        "dataset_size": 50
      },
      "note": "扩展高并行度场景"
    },
    {
      "base_block_id": "CASE_05",
      "priority": "Medium",
      "params": {
        "feature_types": ["SparseFeature", "RaggedFeature"],
        "dtype_combinations": ["int32", "float32"],
        "deterministic": null,
        "dataset_size": 10
      },
      "note": "扩展稀疏和张量特征"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03", "CASE_05"],
  "deferred_set": ["CASE_04", "CASE_06", "CASE_07", "CASE_08"]
}