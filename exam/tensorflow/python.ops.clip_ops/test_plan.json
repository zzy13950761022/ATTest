{
  "plan_version": 2,
  "target": "tensorflow.python.ops.clip_ops",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {"include": "SMOKE_SET", "assert_level": "weak", "max_blocks": 5},
    "roundN": {"only_fix_failed_blocks": true, "block_limit": 3, "promote_deferred": true},
    "final": {"enable_strong_asserts": true, "coverage_optional": true}
  },
  "test_files": {
    "single": "tests/test_tensorflow_python_ops_clip_ops.py",
    "default": "tests/test_tensorflow_python_ops_clip_ops.py",
    "all_pattern": "tests/test_tensorflow_python_ops_clip_ops_*.py"
  },
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "name": "基本裁剪功能_标量裁剪值",
      "priority": "High",
      "param_matrix": [
        {"dtype": "float32", "shape": [2, 3], "clip_min": -1.0, "clip_max": 1.0, "flags": []}
      ],
      "asserts": {
        "weak": ["shape", "dtype", "value_in_range"],
        "strong": ["approx_equal", "exact_values"]
      },
      "oracle": "clip_ops.clip_by_value",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 80,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "name": "广播机制_不同形状裁剪张量",
      "priority": "High",
      "param_matrix": [
        {"dtype": "float32", "shape": [3, 4], "clip_min_shape": [3, 1], "clip_max_shape": [1, 4], "flags": ["broadcast"]}
      ],
      "asserts": {
        "weak": ["shape", "dtype", "broadcast_correct"],
        "strong": ["approx_equal", "broadcast_validation"]
      },
      "oracle": "clip_ops.clip_by_value",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 100,
      "max_params": 8,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "name": "类型兼容性_多种数值类型",
      "priority": "High",
      "param_matrix": [
        {"dtype": "int32", "shape": [2, 2], "clip_min": 0, "clip_max": 10, "flags": ["int_type"]}
      ],
      "asserts": {
        "weak": ["shape", "dtype", "value_in_range"],
        "strong": ["exact_equality", "type_preservation"]
      },
      "oracle": "clip_ops.clip_by_value",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 80,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "name": "边界条件_相等裁剪值",
      "priority": "High",
      "param_matrix": [
        {"dtype": "float32", "shape": [3, 3], "clip_min": 2.5, "clip_max": 2.5, "flags": ["equal_clip"]}
      ],
      "asserts": {
        "weak": ["shape", "dtype", "all_equal"],
        "strong": ["approx_equal", "exact_values"]
      },
      "oracle": "clip_ops.clip_by_value",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 80,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "name": "异常处理_无效裁剪范围",
      "priority": "High",
      "param_matrix": [
        {"dtype": "float32", "shape": [2, 2], "clip_min": 5.0, "clip_max": 1.0, "flags": ["error_case"]}
      ],
      "asserts": {
        "weak": ["exception_type", "exception_message"],
        "strong": ["exception_details", "error_context"]
      },
      "oracle": "clip_ops.clip_by_value",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 80,
      "max_params": 6,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-06",
      "block_id": "CASE_06",
      "name": "IndexedSlices类型输入",
      "priority": "Medium",
      "param_matrix": [
        {"dtype": "float32", "shape": [5, 3], "indices_shape": [3], "values_shape": [3, 3], "clip_min": -1.0, "clip_max": 1.0, "flags": ["indexed_slices"]}
      ],
      "asserts": {
        "weak": ["type_preservation", "shape_preservation", "value_in_range"],
        "strong": ["approx_equal", "indices_preservation"]
      },
      "oracle": "clip_ops.clip_by_value",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 100,
      "max_params": 8,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": [
        "tensorflow.python.ops.math_ops.minimum",
        "tensorflow.python.ops.math_ops.maximum",
        "tensorflow.python.framework.ops.convert_to_tensor",
        "tensorflow.python.framework.tensor_shape.TensorShape",
        "tensorflow.python.ops.array_ops.broadcast_to"
      ]
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {"dtype": "float64", "shape": [4, 4], "clip_min": -2.0, "clip_max": 2.0, "flags": ["float64_type"]},
      "note": "作为参数扩展：float64类型"
    },
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {"dtype": "float32", "shape": [1, 10], "clip_min": 0.0, "clip_max": 100.0, "flags": ["large_range"]},
      "note": "作为参数扩展：大范围裁剪"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Medium",
      "params": {"dtype": "float32", "shape": [2, 3, 4], "clip_min_shape": [2, 1, 4], "clip_max_shape": [1, 3, 1], "flags": ["broadcast", "3d"]},
      "note": "作为参数扩展：三维广播"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {"dtype": "int64", "shape": [3, 2], "clip_min": -100, "clip_max": 100, "flags": ["int64_type"]},
      "note": "作为参数扩展：int64类型"
    },
    {
      "base_block_id": "CASE_04",
      "priority": "Medium",
      "params": {"dtype": "int32", "shape": [2, 2], "clip_min": 0, "clip_max": 0, "flags": ["equal_clip", "int_type"]},
      "note": "作为参数扩展：整数相等裁剪值"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03", "CASE_04", "CASE_05"],
  "deferred_set": ["CASE_06"]
}