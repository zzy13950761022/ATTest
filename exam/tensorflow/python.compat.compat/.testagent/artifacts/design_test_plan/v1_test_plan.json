{
  "plan_version": 2,
  "target": "tensorflow.python.compat.compat",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_compat_compat.py",
    "all_pattern": "tests/test_tensorflow_python_compat_compat_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_compat_compat_g1.py",
      "G2": "tests/test_tensorflow_python_compat_compat_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "forward_compatible函数测试",
      "entrypoints": ["forward_compatible"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05"],
      "note": "测试forward_compatible基本功能与异常处理"
    },
    {
      "group_id": "G2",
      "title": "forward_compatibility_horizon函数测试",
      "entrypoints": ["forward_compatibility_horizon"],
      "smoke_set": ["CASE_03", "CASE_04"],
      "deferred_set": ["CASE_06"],
      "note": "测试上下文管理器功能与状态管理"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "forward_compatible基本功能测试",
      "priority": "High",
      "param_matrix": [
        {
          "year": 2021,
          "month": 12,
          "day": 1,
          "env_delta": 0
        }
      ],
      "asserts": {
        "weak": ["返回布尔值", "类型正确", "无异常"],
        "strong": ["日期编码正确", "基准日期计算正确", "环境变量影响正确"]
      },
      "oracle": "基于当前日期计算的预期结果",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": true
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "forward_compatible无效参数异常测试",
      "priority": "High",
      "param_matrix": [
        {
          "year": "invalid",
          "month": 12,
          "day": 1,
          "expected_exception": "TypeError"
        },
        {
          "year": 2021,
          "month": 13,
          "day": 1,
          "expected_exception": "ValueError"
        },
        {
          "year": 2021,
          "month": 2,
          "day": 30,
          "expected_exception": "ValueError"
        }
      ],
      "asserts": {
        "weak": ["触发指定异常", "异常类型正确", "无其他异常"],
        "strong": ["异常消息包含参数信息", "边界值验证完整"]
      },
      "oracle": "参数验证逻辑",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "forward_compatibility_horizon上下文管理器基本功能",
      "priority": "High",
      "param_matrix": [
        {
          "year": 2021,
          "month": 12,
          "day": 1,
          "env_delta": 0
        }
      ],
      "asserts": {
        "weak": ["上下文管理器可用", "进入时修改全局状态", "退出时恢复状态", "无异常"],
        "strong": ["状态修改正确", "嵌套上下文管理器", "异常时状态恢复"]
      },
      "oracle": "全局状态管理逻辑",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": true
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G2",
      "name": "环境变量影响基准日期测试",
      "priority": "High",
      "param_matrix": [
        {
          "year": 2021,
          "month": 12,
          "day": 1,
          "env_delta": 7,
          "test_type": "forward_compatible"
        },
        {
          "year": 2021,
          "month": 12,
          "day": 1,
          "env_delta": -7,
          "test_type": "forward_compatibility_horizon"
        }
      ],
      "asserts": {
        "weak": ["环境变量影响结果", "不同类型函数受影响", "无异常"],
        "strong": ["环境变量解析正确", "日期计算正确", "边界值处理"]
      },
      "oracle": "环境变量解析逻辑",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "year": 2022,
        "month": 1,
        "day": 15,
        "env_delta": 14
      },
      "note": "不同日期和环境变量组合"
    },
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "year": 2020,
        "month": 6,
        "day": 30,
        "env_delta": -21
      },
      "note": "过去日期测试"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Medium",
      "params": {
        "year": 2021,
        "month": 0,
        "day": 1,
        "expected_exception": "ValueError"
      },
      "note": "月份边界值0"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Medium",
      "params": {
        "year": 2021,
        "month": 12,
        "day": 0,
        "expected_exception": "ValueError"
      },
      "note": "日期边界值0"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {
        "year": 2023,
        "month": 3,
        "day": 15,
        "env_delta": 30
      },
      "note": "未来日期上下文管理器"
    },
    {
      "base_block_id": "CASE_04",
      "priority": "Medium",
      "params": {
        "year": 2021,
        "month": 12,
        "day": 1,
        "env_delta": 0,
        "test_type": "both"
      },
      "note": "无环境变量影响测试"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03", "CASE_04"],
  "deferred_set": ["CASE_05", "CASE_06"]
}