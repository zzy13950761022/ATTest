{
  "plan_version": 2,
  "target": "tensorflow.python.data.experimental.ops.prefetching_ops",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_data_experimental_ops_prefetching_ops.py",
    "all_pattern": "tests/test_tensorflow_python_data_experimental_ops_prefetching_ops_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_data_experimental_ops_prefetching_ops_g1.py",
      "G2": "tests/test_tensorflow_python_data_experimental_ops_prefetching_ops_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "prefetch_to_device核心功能",
      "entrypoints": ["prefetch_to_device"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_03", "CASE_04"],
      "note": "测试prefetch_to_device基本功能与参数验证"
    },
    {
      "group_id": "G2",
      "title": "copy_to_device与设备间传输",
      "entrypoints": ["copy_to_device"],
      "smoke_set": ["CASE_05"],
      "deferred_set": ["CASE_06", "CASE_07"],
      "note": "测试copy_to_device设备间数据传输与GPU支持"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "prefetch_to_device基本功能验证",
      "priority": "High",
      "param_matrix": [
        {
          "device": "/cpu:0",
          "buffer_size": 1,
          "dataset_type": "tensor_slices",
          "data_shape": [10],
          "dtype": "int32"
        }
      ],
      "asserts": {
        "weak": ["returns_callable", "dataset_apply_compatible", "output_shape_match", "output_dtype_match"],
        "strong": ["device_transfer_correct", "data_integrity", "performance_within_bounds"]
      },
      "oracle": "tf.data.Dataset.apply",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "prefetch_to_device buffer_size参数验证",
      "priority": "High",
      "param_matrix": [
        {
          "device": "/cpu:0",
          "buffer_size": null,
          "dataset_type": "tensor_slices",
          "data_shape": [5],
          "dtype": "float32"
        }
      ],
      "asserts": {
        "weak": ["returns_callable", "dataset_apply_compatible", "auto_buffer_size_works", "no_crash"],
        "strong": ["optimal_buffer_selection", "memory_usage_within_limits"]
