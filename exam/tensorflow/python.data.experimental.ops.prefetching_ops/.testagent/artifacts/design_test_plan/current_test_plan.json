        "strong": ["gpu_memory_usage", "transfer_performance", "initializable_iterator_required"]
      },
      "oracle": "tf.data.Dataset.apply",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 75,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true
