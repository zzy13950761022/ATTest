      "block_id": "CASE_05",
      "group_id": "G2",
      "name": "copy_to_device基本功能验证",
      "priority": "High",
      "param_matrix": [
        {
          "target_device": "/cpu:0",
          "source_device": "/cpu:0",
          "dataset_type": "tensor_slices",
