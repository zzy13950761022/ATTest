{
  "plan_version": 2,
  "target": "tensorflow.python.util.nest",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_util_nest.py",
    "all_pattern": "tests/test_tensorflow_python_util_nest.py"
  },
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "name": "flatten基本嵌套结构",
      "priority": "High",
      "param_matrix": [
        {
          "structure": [[1, 2], [3, 4]],
          "expected": [1, 2, 3, 4],
          "description": "列表嵌套列表"
        },
        {
          "structure": {"a": 1, "b": 2},
          "expected": [1, 2],
          "description": "简单字典"
        },
        {
          "structure": ([1, 2], {"x": 3}),
          "expected": [1, 2, 3],
          "description": "元组和字典混合"
        }
      ],
      "asserts": {
        "weak": ["equal_length", "element_order", "basic_property"],
        "strong": ["exact_match", "type_preservation", "edge_cases"]
      },
      "oracle": "tf.nest.flatten",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "name": "map_structure函数应用",
      "priority": "High",
      "param_matrix": [
        {
          "func": "lambda x: x * 2",
          "structure": {"a": 1, "b": 2},
          "expected": {"a": 2, "b": 4},
          "description": "字典乘以2"
        },
        {
          "func": "lambda x: x + 1",
          "structure": [1, [2, 3]],
          "expected": [2, [3, 4]],
          "description": "嵌套列表加1"
        },
        {
          "func": "str",
          "structure": (1, 2, 3),
          "expected": ("1", "2", "3"),
          "description": "元组元素转字符串"
        }
      ],
      "asserts": {
        "weak": ["structure_preserved", "func_applied", "basic_property"],
        "strong": ["exact_structure_match", "func_composition", "error_handling"]
      },
      "oracle": "tf.nest.map_structure",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "name": "assert_same_structure验证",
      "priority": "High",
      "param_matrix": [
        {
          "struct1": [1, 2, 3],
          "struct2": ["a", "b", "c"],
          "should_pass": true,
          "check_types": false,
          "description": "相同结构不同类型"
        },
        {
          "struct1": {"x": 1, "y": 2},
          "struct2": {"x": 3, "y": 4},
          "should_pass": true,
          "check_types": true,
          "description": "相同字典结构"
        },
        {
          "struct1": [1, [2, 3]],
          "struct2": [4, [5, 6]],
          "should_pass": true,
          "check_types": true,
          "description": "嵌套列表结构"
        }
      ],
      "asserts": {
        "weak": ["no_exception_when_valid", "basic_validation"],
        "strong": ["exception_type", "error_message", "type_checking"]
      },
      "oracle": "tf.nest.assert_same_structure",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "name": "pack_sequence_as结构重建",
      "priority": "High",
      "param_matrix": [
        {
          "structure": [0, 0],
          "flat_sequence": [1, 2],
          "expected": [1, 2],
          "description": "简单列表重建"
        },
        {
          "structure": {"a": 0, "b": 0},
          "flat_sequence": [3, 4],
          "expected": {"a": 3, "b": 4},
          "description": "字典重建"
        },
        {
          "structure": [1, [2, 3]],
          "flat_sequence": [10, 20, 30],
          "expected": [10, [20, 30]],
          "description": "嵌套结构重建"
        }
      ],
      "asserts": {
        "weak": ["structure_match", "element_order", "basic_property"],
        "strong": ["exact_reconstruction", "edge_cases", "error_handling"]
      },
      "oracle": "tf.nest.pack_sequence_as",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "name": "is_nested嵌套判断",
      "priority": "High",
      "param_matrix": [
        {
          "structure": [1, 2, 3],
          "expected": true,
          "description": "列表是嵌套结构"
        },
        {
          "structure": {"a": 1},
          "expected": true,
          "description": "字典是嵌套结构"
        },
        {
          "structure": 42,
          "expected": false,
          "description": "整数不是嵌套结构"
        },
        {
          "structure": "string",
          "expected": false,
          "description": "字符串不是嵌套结构"
        }
      ],
      "asserts": {
        "weak": ["boolean_result", "basic_types"],
        "strong": ["edge_cases", "special_types", "consistency"]
      },
      "oracle": "tf.nest.is_nested",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 50,
      "max_params": 3,
      "is_parametrized": true,
      "requires_mock": false,
      "mock_targets": []
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "structure": [],
        "expected": [],
        "description": "空列表"
      },
      "note": "作为参数扩展 - 边界值"
    },
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "structure": {},
        "expected": [],
        "description": "空字典"
      },
      "note": "作为参数扩展 - 边界值"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Medium",
      "params": {
        "func": "lambda x: None",
        "structure": [1, 2, 3],
        "expected": [None, None, None],
        "description": "函数返回None"
      },
      "note": "作为参数扩展 - 特殊函数"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {
        "struct1": [1, 2],
        "struct2": [1, 2, 3],
        "should_pass": false,
        "check_types": true,
        "description": "不同长度结构"
      },
      "note": "作为参数扩展 - 错误场景"
    },
    {
      "base_block_id": "CASE_04",
      "priority": "Medium",
      "params": {
        "structure": (0, 0),
        "flat_sequence": [1, 2],
        "expected": (1, 2),
        "description": "元组重建"
      },
      "note": "作为参数扩展 - 元组类型"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03"],
  "deferred_set": ["CASE_04", "CASE_05"]
}