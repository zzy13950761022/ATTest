        "strong": ["exact_values", "order_preservation"]
      },
      "oracle": "numpy.split",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": [
        "tensorflow.python.ops.gen_array_ops.unpack",
