{
  "plan_version": 2,
  "target": "tensorflow.python.ops.array_ops",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_ops_array_ops.py",
    "all_pattern": "tests/test_tensorflow_python_ops_array_ops_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_ops_array_ops_g1.py",
      "G2": "tests/test_tensorflow_python_ops_array_ops_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心形状操作组",
      "entrypoints": ["reshape", "expand_dims"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05"],
      "note": "测试张量形状变换和维度操作"
    },
    {
      "group_id": "G2",
      "title": "张量组合操作组",
      "entrypoints": ["concat", "stack", "unstack"],
      "smoke_set": ["CASE_03", "CASE_04"],
      "deferred_set": ["CASE_06"],
      "note": "测试张量连接、堆叠和解堆叠操作"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "reshape基本形状变换",
      "priority": "High",
      "param_matrix": [
        {
          "func": "reshape",
          "input_shape": [2, 3],
          "target_shape": [6],
          "dtype": "float32",
          "device": "cpu",
          "flags": []
        }
      ],
      "asserts": {
        "weak": ["shape", "dtype", "element_count", "basic_property"],
        "strong": ["exact_values", "memory_layout"]
      },
      "oracle": "numpy.reshape",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": [
        "tensorflow.python.ops.gen_array_ops.reshape",
        "tensorflow.python.framework.tensor_util.maybe_set_static_shape",
        "tensorflow.python.framework.ops.convert_to_tensor"
      ]
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
