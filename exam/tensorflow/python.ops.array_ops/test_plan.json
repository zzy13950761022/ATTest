      "asserts": {
        "weak": ["shape", "dtype", "element_count", "inferred_dimension"],
        "strong": ["exact_values", "inference_correctness"]
      },
      "oracle": "numpy.reshape",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 5,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": [
        "tensorflow.python.ops.gen_array_ops.reshape",
        "tensorflow.python.framework.tensor_util.maybe_set_static_shape",
        "tensorflow.python.framework.ops.convert_to_tensor"
      ]
    },
    {
      "tc_id": "TC-06",
      "block_id": "CASE_06",
