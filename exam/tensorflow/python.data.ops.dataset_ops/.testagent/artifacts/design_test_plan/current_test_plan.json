{
  "plan_version": 2,
  "target": "tensorflow.python.data.ops.dataset_ops",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_tensorflow_python_data_ops_dataset_ops.py",
    "all_pattern": "tests/test_tensorflow_python_data_ops_dataset_ops_*.py",
    "groups": {
      "G1": "tests/test_tensorflow_python_data_ops_dataset_ops_g1.py",
      "G2": "tests/test_tensorflow_python_data_ops_dataset_ops_g2.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "数据集创建与基本操作",
      "entrypoints": ["DatasetV2", "from_tensor_slices", "range"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05"],
      "note": "测试数据集创建、元素规范和基本迭代"
    },
    {
      "group_id": "G2",
      "title": "数据转换操作",
      "entrypoints": ["map", "batch", "shuffle", "filter"],
      "smoke_set": ["CASE_03", "CASE_04"],
      "deferred_set": ["CASE_06", "CASE_07"],
      "note": "测试map、batch、shuffle等转换操作"
    }
  ],
  "cases": [
