      "name": "从张量切片创建数据集",
      "priority": "High",
      "param_matrix": [
        {
          "data_type": "tensor",
          "data_shape": [5, 3],
          "dtype": "float32",
          "device": "cpu",
          "batch_size": null,
          "shuffle": false
        }
      ],
      "asserts": {
        "weak": ["element_spec_exists", "cardinality_correct", "iteration_complete", "shape_preserved"],
        "strong": ["exact_values_match", "memory_efficient", "type_strict"]
      },
      "oracle": "tf.data.Dataset.from_tensor_slices",
      "assertion_level": "weak",
      "size": "S",
