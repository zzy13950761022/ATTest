      "param_matrix": [
        {
          "data_type": "list",
          "data_shape": [10],
          "dtype": "int32",
          "device": "cpu",
          "batch_size": null,
          "shuffle": false
        }
      ],
      "asserts": {
        "weak": ["element_spec_exists", "cardinality_correct", "iteration_complete", "values_preserved"],
        "strong": ["type_conversion_correct", "performance_acceptable", "memory_usage"]
