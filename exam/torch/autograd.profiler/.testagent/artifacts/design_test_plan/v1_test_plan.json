      ],
      "asserts": {
        "weak": ["context_manager_works", "profiler_created", "events_recorded", "no_exception"],
        "strong": ["event_count_positive", "cpu_time_measured", "table_format_correct", "key_averages_works"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 6,
      "is_parametrized": false,
      "requires_mock": false
