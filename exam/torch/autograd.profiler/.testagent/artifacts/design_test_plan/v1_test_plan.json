{
  "plan_version": 2,
  "target": "torch.autograd.profiler",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_torch_autograd_profiler.py",
    "all_pattern": "tests/test_torch_autograd_profiler_*.py",
    "groups": {
      "G1": "tests/test_torch_autograd_profiler_basic.py",
      "G2": "tests/test_torch_autograd_profiler_advanced.py",
      "G3": "tests/test_torch_autograd_profiler_edge.py"
    }
  },
  "active_group_order": ["G1", "G2", "G3"],
  "groups": [
    {
      "group_id": "G1",
      "title": "基础功能测试",
      "entrypoints": ["profile", "__enter__", "__exit__"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_04"],
      "note": "测试 profile 上下文管理器的基本功能"
    },
    {
      "group_id": "G2",
      "title": "高级配置测试",
      "entrypoints": ["key_averages", "table", "export_chrome_trace"],
      "smoke_set": ["CASE_03"],
      "deferred_set": ["CASE_05", "CASE_06"],
      "note": "测试不同配置参数组合和输出方法"
    },
    {
      "group_id": "G3",
      "title": "边界与异常测试",
      "entrypoints": ["record_function", "emit_nvtx"],
      "smoke_set": [],
      "deferred_set": ["CASE_07", "CASE_08", "CASE_09"],
      "note": "测试异常场景、边界条件和相关类"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "基本 CPU 分析",
      "priority": "High",
      "param_matrix": [
        {
          "enabled": true,
          "use_cuda": false,
          "record_shapes": false,
          "with_flops": false,
          "profile_memory": false,
          "with_stack": false,
          "with_modules": false,
          "use_kineto": false,
          "use_cpu": true,
          "experimental_config": null
