{
  "plan_version": 2,
  "target": "torch.cuda.nvtx",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_torch_cuda_nvtx.py",
    "all_pattern": "tests/test_torch_cuda_nvtx_*.py",
    "groups": {
      "G1": "tests/test_torch_cuda_nvtx_basic.py",
      "G2": "tests/test_torch_cuda_nvtx_range.py",
      "G3": "tests/test_torch_cuda_nvtx_context.py"
    }
  },
  "active_group_order": ["G1", "G2", "G3"],
  "groups": [
    {
      "group_id": "G1",
      "title": "基础函数测试",
      "entrypoints": ["range_push", "range_pop", "mark"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05"],
      "note": "测试基本范围栈操作和事件标记"
    },
    {
      "group_id": "G2",
      "title": "跨线程范围管理",
      "entrypoints": ["range_start", "range_end"],
      "smoke_set": ["CASE_03"],
      "deferred_set": ["CASE_06"],
      "note": "测试跨线程范围句柄管理"
    },
    {
      "group_id": "G3",
      "title": "上下文管理器",
      "entrypoints": ["range"],
      "smoke_set": ["CASE_04"],
      "deferred_set": ["CASE_07"],
      "note": "测试range上下文管理器功能"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "基本范围栈操作",
      "priority": "High",
      "param_matrix": [
        {
          "msg": "test_range",
          "depth": 1,
          "use_ascii": true
        }
      ],
      "asserts": {
        "weak": ["range_push_returns_int", "range_pop_returns_int", "depth_consistent", "no_exception"],
        "strong": ["exact_depth_match", "stack_integrity", "thread_safety"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "事件标记功能",
      "priority": "High",
      "param_matrix": [
        {
          "msg": "test_mark",
          "use_ascii": true
        }
      ],
      "asserts": {
        "weak": ["no_exception", "function_executes", "returns_none_or_int"],
        "strong": ["mark_registered", "timing_correct", "event_visible"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 50,
      "max_params": 3,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "跨线程范围句柄",
      "priority": "High",
      "param_matrix": [
        {
          "msg": "cross_thread_range",
          "use_ascii": true
        }
      ],
      "asserts": {
        "weak": ["range_start_returns_int", "range_end_no_exception", "handle_valid", "no_exception"],
        "strong": ["handle_uniqueness", "cross_thread_consistency", "resource_cleanup"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 70,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G3",
      "name": "上下文管理器基本使用",
      "priority": "High",
      "param_matrix": [
        {
          "msg": "context_range",
          "use_ascii": true,
          "use_context": true
        }
      ],
      "asserts": {
        "weak": ["context_enters", "context_exits", "no_exception", "returns_context"],
        "strong": ["context_timing", "nested_context", "exception_handling"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 65,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": false
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "group_id": "G1",
      "name": "CUDA不可用异常",
      "priority": "Medium",
      "param_matrix": [
        {
          "msg": "test_error",
          "cuda_available": false
        }
      ],
      "asserts": {
        "weak": ["raises_runtime_error", "error_message_contains", "no_side_effects"],
        "strong": ["error_type_specific", "clean_state_after_error", "recovery_possible"]
      },
      "oracle": "manual_verification",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 55,
      "max_params": 3,
      "is_parametrized": true,
      "requires_mock": true
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "msg": "nested_range",
        "depth": 3,
        "use_ascii": true
      },
      "note": "多级嵌套范围测试"
    },
    {
      "base_block_id": "CASE_01",
      "priority": "Low",
      "params": {
        "msg": "empty_string",
        "depth": 1,
        "use_ascii": true
      },
      "note": "空字符串参数测试"
    },
    {
      "base_block_id": "CASE_02",
      "priority": "Medium",
      "params": {
        "msg": "long_ascii_string",
        "use_ascii": true
      },
      "note": "长ASCII字符串测试"
    },
    {
      "base_block_id": "CASE_03",
      "priority": "Medium",
      "params": {
        "msg": "multiple_ranges",
        "use_ascii": true,
        "count": 5
      },
      "note": "多个并发范围测试"
    },
    {
      "base_block_id": "CASE_04",
      "priority": "Medium",
      "params": {
        "msg": "formatted_range_{}",
        "use_ascii": true,
        "use_context": true,
        "format_args": ["test"]
      },
      "note": "格式化参数测试"
    }
  ],
  "smoke_set": ["CASE_01", "CASE_02", "CASE_03", "CASE_04"],
  "deferred_set": ["CASE_05", "CASE_06", "CASE_07"]
}