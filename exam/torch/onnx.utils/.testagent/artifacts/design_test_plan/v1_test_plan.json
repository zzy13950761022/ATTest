{
  "plan_version": 2,
  "target": "torch.onnx.utils",
  "block_rules": {
    "header_block": "HEADER",
    "footer_block": "FOOTER",
    "case_prefix": "CASE_",
    "case_format": "CASE_01"
  },
  "iteration_strategy": {
    "round1": {
      "include": "SMOKE_SET",
      "assert_level": "weak",
      "max_blocks": 5
    },
    "roundN": {
      "only_fix_failed_blocks": true,
      "block_limit": 3,
      "promote_deferred": true
    },
    "final": {
      "enable_strong_asserts": true,
      "coverage_optional": true
    }
  },
  "test_files": {
    "default": "tests/test_torch_onnx_utils.py",
    "all_pattern": "tests/test_torch_onnx_utils_*.py",
    "groups": {
      "G1": "tests/test_torch_onnx_utils_export.py",
      "G2": "tests/test_torch_onnx_utils_helpers.py"
    }
  },
  "active_group_order": ["G1", "G2"],
  "groups": [
    {
      "group_id": "G1",
      "title": "核心导出函数族",
      "entrypoints": ["export", "export_to_pretty_string"],
      "smoke_set": ["CASE_01", "CASE_02"],
      "deferred_set": ["CASE_05", "CASE_06", "CASE_07"],
      "note": "测试模型导出到文件和字符串的核心功能"
    },
    {
      "group_id": "G2",
      "title": "辅助函数与状态管理",
      "entrypoints": ["is_in_onnx_export", "_decide_keep_init_as_input"],
      "smoke_set": ["CASE_03"],
      "deferred_set": ["CASE_08", "CASE_09"],
      "note": "测试导出状态管理和辅助决策函数"
    }
  ],
  "cases": [
    {
      "tc_id": "TC-01",
      "block_id": "CASE_01",
      "group_id": "G1",
      "name": "基本模型导出到文件",
      "priority": "High",
      "param_matrix": [
        {
          "model_type": "nn.Module",
          "args_format": "tuple",
          "output_target": "file",
          "opset_version": 13,
          "export_params": true,
          "do_constant_folding": true
        }
      ],
      "asserts": {
        "weak": ["file_exists", "file_not_empty", "no_exception"],
        "strong": ["onnx_format_valid", "model_structure_preserved", "tensor_values_approx_equal"]
      },
      "oracle": "torch.onnx.export",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 100,
      "max_params": 8,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": ["torch.jit.trace", "io.open", "torch.onnx._internals._model_to_graph"]
    },
    {
      "tc_id": "TC-02",
      "block_id": "CASE_02",
      "group_id": "G1",
      "name": "模型导出到BytesIO缓冲区",
      "priority": "High",
      "param_matrix": [
        {
          "model_type": "nn.Module",
          "args_format": "tensor",
          "output_target": "bytesio",
          "opset_version": 13,
          "export_params": true,
          "do_constant_folding": true
        }
      ],
      "asserts": {
        "weak": ["buffer_not_empty", "buffer_position_changed", "no_exception"],
        "strong": ["buffer_content_valid", "onnx_protobuf_parseable", "model_metadata_present"]
      },
      "oracle": "torch.onnx.export",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 90,
      "max_params": 7,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": ["torch.jit.trace", "torch.onnx._internals._model_to_graph"]
    },
    {
      "tc_id": "TC-03",
      "block_id": "CASE_03",
      "group_id": "G2",
      "name": "导出状态检测函数",
      "priority": "High",
      "param_matrix": [
        {
          "export_state": "inside_export",
          "expected_result": true
        },
        {
          "export_state": "outside_export",
          "expected_result": false
        }
      ],
      "asserts": {
        "weak": ["function_returns_bool", "no_exception", "state_transition_correct"],
        "strong": ["thread_safe_check", "nested_export_state", "exception_state_cleanup"]
      },
      "oracle": "torch.onnx.utils.is_in_onnx_export",
      "assertion_level": "weak",
      "size": "S",
      "max_lines": 60,
      "max_params": 4,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": ["torch.onnx.utils.GLOBALS"]
    },
    {
      "tc_id": "TC-04",
      "block_id": "CASE_04",
      "group_id": "G1",
      "name": "ScriptModule模型导出",
      "priority": "High",
      "param_matrix": [
        {
          "model_type": "ScriptModule",
          "args_format": "tuple",
          "output_target": "file",
          "opset_version": 13,
          "export_params": true,
          "do_constant_folding": true
        }
      ],
      "asserts": {
        "weak": ["file_exists", "no_trace_called", "no_exception"],
        "strong": ["script_module_preserved", "torchscript_compatibility", "graph_optimization_applied"]
      },
      "oracle": "torch.onnx.export",
      "assertion_level": "weak",
      "size": "M",
      "max_lines": 95,
      "max_params": 7,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": ["torch.onnx._internals._model_to_graph", "io.open"]
    },
    {
      "tc_id": "TC-05",
      "block_id": "CASE_05",
      "group_id": "G1",
      "name": "动态轴配置导出",
      "priority": "Medium",
      "param_matrix": [
        {
          "model_type": "nn.Module",
          "args_format": "tuple",
          "output_target": "file",
          "opset_version": 13,
          "dynamic_axes_format": "dict",
          "export_params": true
        }
      ],
      "asserts": {
        "weak": ["file_exists", "dynamic_axes_applied", "no_exception"],
        "strong": ["dynamic_dimensions_present", "shape_inference_correct", "multiple_dynamic_axes"]
      },
      "oracle": "torch.onnx.export",
      "assertion_level": "weak",
      "size": "L",
      "max_lines": 120,
      "max_params": 9,
      "is_parametrized": true,
      "requires_mock": true,
      "mock_targets": ["torch.jit.trace", "torch.onnx._internals._model_to_graph", "io.open"]
    }
  ],
  "param_extensions": [
    {
      "base_block_id": "CASE_01",
      "priority": "Medium",
      "params": {
        "model_type": "nn.Module",
        "args_format": "named_tuple",
